<section class='p-4 bg-light'>

  
  <h1 class='text-secondary text-center text-success'>GRACIAS POR REGISTRARTE!</h1>

  <div class='mb-3'>
    <div>
      <label class='form-label fw-bold text-secondary fs-3' for='login'>Tus datos son:</label>
      <div>
        usuario:
        {{user.username}}
        <br />
        Direccion:
        {{user.address}}
        <br />
        Edad:
        {{user.age}}
        <br />
        Telefono:
        {{user.phone}}
        <br />
        Imagen:
        {{user.url}}
      </div>
    </div>
    <br />
    <button type='button' class='btn btn-danger'><a href='./logout' style='text-decoration: none;'>Salir</a></button>
    <button type='button' class='btn btn-success'><a href='/login' style='text-decoration: none;'>Home</a></button>

  </div>
</section>